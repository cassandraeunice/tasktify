@{
    ViewData["Title"] = "User Profile";
    Layout = "_Layout";
}

<style>
    h3 {
        font-size: 70px;
        color: #000;
        text-shadow: 0px 4px 4px rgba(0, 0, 0, 0.25);
        margin-left: 30px;
        margin-top: 10px;
        margin-bottom: 2px;
    }

    .userProfile {
        margin-left: 100px;
        margin-top: 2px;
        display: flex;
        align-items: center;
    }

    #userImage {
        margin-right: 20px;
    }

    #userInfo {
        margin-left: 10px;
        display: flex;
        flex-direction: column;
        align-items: center;
    }

    .userInfoItem {
        margin-top: 1px;
        font-size: 28px;
    }


    hr {
        display: block;
        margin-top: 10px;
        margin-bottom: 0.5em;
        margin-right: auto;
        border-style: inset;
        border-width: 3px;
        margin-left: 100px;
    }

    #formContainer {
        margin-left: 100px;
        margin-top: 10px;
    }

    #firstName label {
        font-size: 20px;
        font-weight: bold;
    }

    #firstName input {
        height: 30px;
        width: 450px;
        border-radius: 10px;
        box-shadow: 0px 4px 4px 0px rgba(0, 0, 0, 0.25);
    }

    #lastName {
        margin-left: 350px;
        margin-top: -65.5px;
    }

        #lastName label {
            font-size: 20px;
            font-weight: bold;
            margin-left: 150px;
        }

        #lastName input {
            height: 30px;
            width: 450px;
            margin-left: 150px;
            border-radius: 10px;
            box-shadow: 0px 4px 4px 0px rgba(0, 0, 0, 0.25);
        }

    #email label {
        font-size: 20px;
        font-weight: bold;
    }

    #email input {
        height: 30px;
        width: 950px;
        border-radius: 10px;
        box-shadow: 0px 4px 4px 0px rgba(0, 0, 0, 0.25);
    }

    #newPassword label {
        font-size: 20px;
        font-weight: bold;
    }

    #newPassword input {
        height: 30px;
        width: 450px;
        border-radius: 10px;
        box-shadow: 0px 4px 4px 0px rgba(0, 0, 0, 0.25);
    }

    #confPassword {
        margin-left: 350px;
        margin-top: -65.5px;
    }

        #confPassword label {
            font-size: 20px;
            font-weight: bold;
            margin-left: 150px;
        }

        #confPassword input {
            height: 30px;
            width: 450px;
            border-radius: 10px;
            margin-left: 150px;
            box-shadow: 0px 4px 4px 0px rgba(0, 0, 0, 0.25);
        }

    #save {
        height: 40px;
        width: 70px;
        font-size: 18px;
        border: none;
        border-radius: 10px;
        background: #6DBA8B;
    }

    #cancel {
        height: 40px;
        width: 90px;
        font-size: 18px;
        border: none;
        border-radius: 10px;
        background: #D5D5D5;
        margin-left: 50px;
    }

    #logout {
        height: 40px;
        width: 90px;
        font-size: 18px;
        border: none;
        border-radius: 10px;
        background: #D45F57;
    }

    .logout-link {
        color: black;
        text-decoration: none;
    }

    }
</style>
<h3>User Profile</h3>
<div class="userProfile">
    <div id="userImage">
        <svg width="188" height="188" viewBox="0 0 188 188" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
            <rect width="188" height="188" fill="url(#pattern0)" />
            <defs>

                <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
                    <use xlink:href="#image0_369_261" transform="scale(0.00531915)" />
                </pattern>
                <image id="image0_369_261" width="188" height="188" xlink:href="data:image/png;base64," />
            </defs>
        </svg>

    </div>
    <div id="userInfo">
        <div class="userInfoItem" id="userName">
            <p>Name</p>
        </div>
        <div class="userInfoItem" id="userEmail">
            <p>Email</p>
        </div>
    </div>
</div>

<hr />





<div id="formContainer">
    <form asp-action="UserProfile" asp-controller="Tasktify" method="post">
        <div id="firstName">
            <label>First Name</label> <br />
            <input type="text" />
        </div>
        <div id="lastName">
            <label>Last Name</label> <br />
            <input type="text" />
        </div> <br />
        <div id="email">
            <label>Email</label> <br />
            <input type="text" />
        </div> <br />
        <div id="newPassword">
            <label>New Password</label> <br />
            <input type="text" />
        </div>
        <div id="confPassword">
            <label>Confirm Password</label> <br />
            <input type="text" />
        </div> <br />
        <div id="buttons">
            <button id="save">Save</button>
            <button id="cancel">Cancel</button>
            <br /><br /><br />
            <button id="logout">
                <a href="@Url.Action("LoginPage")" target="_blank" class="logout-link">Logout</a>
            </button>
        </div>

    </form>
</div>

